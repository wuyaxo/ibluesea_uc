<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 配置mysql 驱动jar包路径.用了绝对路径 -->
	<classPathEntry location="D:\MySQL\mysql-connector-java-5.1.35-bin.jar" />

	<context id="ibluesea_uc">

		<!-- 为了防止生成的代码中有很多注释，比较难看，加入下面的配置控制 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="false" />
		</commentGenerator>
		<!-- 注释控制完毕 -->

		<jdbcConnection connectionURL="jdbc:mysql://node1:3306/uc_test51" driverClass="com.mysql.jdbc.Driver" password="root"
			userId="root" />
		<javaModelGenerator targetPackage="cn.ibluesea.uc.model" targetProject="uc">
			<property name="enableSubPackages" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="cn.ibluesea.uc.dao" targetProject="uc">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator targetPackage="cn.ibluesea.uc.dao" targetProject="uc" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<table schema="uc_test51" tableName="uc_user" domainObjectName="User" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="true"/>
			<ignoreColumn column="CreateTime" />
			<ignoreColumn column="UpdateTime" />
		</table>
	</context>
</generatorConfiguration>